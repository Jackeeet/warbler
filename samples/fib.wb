-- -> wb.IO (read, print)
-- -> wb.IO except someFunc

\ fib(int n) int :>
    if n == 0 then :> 1 <:
    else if n == 1 then :> 1 <:
    else :>
       fib(n - 1) + fib(n - 2)
    <:
   --  n == 0 ? 1 : n == 1 ? 1 : fib(n - 1) + fib(n - 2)
<:

\ run() int :>
   // print("Enter a number: ")
   // def num = int(read())
   // print(fib(num))    -- print(num.fib()) is also valid
   
   def num = 5
   fib(num)
   -- fib(5) 
<:

run()


