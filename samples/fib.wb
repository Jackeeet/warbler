-> wb.System (parseInt)
-- -> wb.System hiding SomeMethod


fn run() :>
    pLine("Enter a number:")
    def num = parseInt(rLine())
    pLine(fib(num))
<: void

fn fib(int num) :>
    if num >= 1 then :>
        ret num
    <:
    else :>
        ret fib(num - 1) + fib(num - 2)
    <:
<: int
